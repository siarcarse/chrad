{
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module",
    },
    "rules": {
        "brace-style": [2, "stroustrup"],
        "camelcase": 1,
        "new-cap": 0,
        "semi": [2, "always"],
        "quotes": [2, "single"],
        "consistent-this": [2, "self"],
        "func-style": [2, "expression"],
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-lonely-if": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "semi-spacing": 2,
        "no-trailing-spaces": 1,
        "no-underscore-dangle": 1,
        "no-mixed-spaces-and-tabs": 2,
        "sort-vars": 1,
        "object-curly-spacing": [1, "always"],
        "space-infix-ops": 2,
        "keyword-spacing": 2,
        "space-unary-ops": 2,
        "no-unused-vars": 1,
        "no-use-before-define": 2,
        "eqeqeq": 2,
        "curly": [2, "all"],
        "dot-notation": 1,
        "no-eval": 2,
        "no-else-return": 2,
        "no-eq-null": 1,
        "no-extend-native": 1,
        "yoda": [1, "never"],
        "no-return-assign": 2,
        "no-new-wrappers": 2,
        "no-native-reassign": 2,
        "no-constant-condition": 2,
        "comma-dangle": 2,
        "no-sparse-arrays": 2,
        "no-ex-assign": 2,
        "indent": [
            "error",
            4
        ]
    }
}
