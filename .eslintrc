{
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module",
    },
    "rules": {
        "brace-style": [2, "stroustrup"],
        "camelcase": 1,
        "new-cap": 0,
        "semi": [1, "always"],
        "quotes": [1, "single"],
        "consistent-this": [1, "self"],
        "func-style": [1, "expression"],
        "new-parens": 1,
        "no-array-constructor": 1,
        "no-lonely-if": 1,
        "no-new-object": 1,
        "no-spaced-func": 1,
        "semi-spacing": 1,
        "no-trailing-spaces": 1,
        "no-underscore-dangle": 1,
        "no-mixed-spaces-and-tabs": 1,
        "sort-vars": 1,
        "object-curly-spacing": [1, "always"],
        "space-infix-ops": 1,
        "keyword-spacing": 1,
        "space-unary-ops": 1,
        "no-unused-vars": 1,
        "no-use-before-define": 1,
        "eqeqeq": 1,
        "curly": [1, "all"],
        "dot-notation": 1,
        "no-eval": 1,
        "no-else-return": 1,
        "no-eq-null": 1,
        "no-extend-native": 1,
        "yoda": [1, "never"],
        "no-return-assign": 1,
        "no-new-wrappers": 1,
        "no-native-reassign": 1,
        "no-constant-condition": 1,
        "comma-dangle": 1,
        "no-sparse-arrays": 1,
        "no-ex-assign": 1,
        "indent": [
            "warn",
            4
        ]
    }
}
